  },
  "engines": {
    "node": "12.x",
    "npm": "6.x",
    "yarn": "1.19.1"
  },
  "workspaces": {
    "nohoist": [
      "pre-commit",
      "lint-staged"
    ]
  },
  "pre-commit": {
    "silent": true,
    "colors": true,
    "run": [
      "lint:staged"
    ]
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "test": "jest --config ./src/tools/jest/jest.config.js",
    "test:watch": "jest --watch --config ./src/tools/jest/jest.config.js",
    "test:clear-cache": "jest --clear-cache --config ./src/tools/jest/jest.config.js",
    "test:coverage": "jest --coverage --colors --config ./src/tools/jest/jest.config.js",
    "lint": "eslint ./src && eslint ./*.js",
    "lint:fix": "eslint ./src --fix && eslint ./*.js --fix",
    "lint:staged": "lint-staged --shell",
    "clean": "yarn clean:dist && clean:lock && clean:modules",
    "clean:dist": "gatsby clean",
    "clean:lock": "rm -rf yarn.lock",
    "clean:modules": "rm -rf node_modules",
    "bundle-report": "webpack-bundle-analyzer --host localhost --port 2000 public/stats.json",
    "build": "cross-env GATSBY_DEPLOY_BUILD=true gatsby build",
    "build:local": "gatsby build",
    "serve": "yarn clean:dist && yarn build:local && gatsby serve -o --port 4000",
    "start": "gatsby develop -o --port 3000"
  },
